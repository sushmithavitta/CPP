#include<bits/stdc++.h>
using namespace std;
int find(vector<int>& arr,int n){
    for(int i=1;i<=n;i++){
        int flag=0;
        for(int j=0;j<n;j++){
        if(arr[j]==i){
            flag=1;
            break;
            
        }
        }
        if(flag==0){
            return i;
        }
        
        
}
 return n+1;
}
int main(){
    int n;
    n=5
    vector<int> arr={1,2,4,5};
    //cin >>n;
    //vector<int> arr(n);
    //for(int i=0;i<n;i++){
      //  cin>> arr[i];
    //}
    
    int i=find(arr,n);
    cout<<i;
    return 0;
}
