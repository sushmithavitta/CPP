#include<bits/stdc++.h>
using namespace std;
int find(int arr[],int n){
    int result=0;
    for(int i=0;i<n;i++){
        result=result^arr[i];
        
    }
    return result;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int res=find(arr,n);
    cout<<res;
    return 0;
    
}

